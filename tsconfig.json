{
  "compilerOptions": {
    "jsx": "react-native",
    "baseUrl": ".",
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext"],
    "checkJs": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "plugins": [],
    "paths": {
      "@screens/*": ["./screens/*"],
      "@screens": ["./screens"],
      "@components/*": ["./components/*"],
      "@api-client/*": ["./api-client/*"],
      "@api-client": ["./api-client"],
      "@stacks/*": ["./stacks/*"],
      "@hooks/*": ["./hooks/*"],
      "@assets/*": ["./assets/*"],
      "@graphql/*": ["./src/graphql/*"],
      "@lib/*": ["./lib/*"],
      "@core-auth/*": ["./core-auth/*"],
      "@core-auth": ["./core-auth"],
      "@modules/*": ["./modules/*"],
      "@core-event-details/*": ["./core-event-details/*"],
      "@core-event-details": ["./core-event-details"],
      "@core-root/*": ["./core-root/*"],
      "@core-root": ["./core-root"],
      "@location/*": ["./location/*"],
      "@location": ["./location"],
      "@content-parsing": ["./content-parsing"],
      "@core-content-reporting": ["./core-content-reporting"],
      "@date-time": ["./date-time"],
      "@date-time/*": ["./date-time/*"],
      "@test-helpers/*": ["./test-helpers/*"],
      "@shared-models/*": ["./shared-models/*"],
      "@notifications": ["./notifications"],
      "@core-explore-events": ["./core-explore-events"],
      "@arrival-tracking": ["./arrival-tracking"],
      "@arrival-tracking/*": ["./arrival-tracking/*"],
      "env": ["./.env"]
    }
  },
  "extends": "expo/tsconfig.base",
  "exclude": ["node_modules", "amplify/#current-cloud-backend", "dist"]
}
