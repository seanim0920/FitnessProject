{
  "compilerOptions": {
    "jsx": "react-native",
    "baseUrl": ".",
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext"],
    "checkJs": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "plugins": [],
    "paths": {
      "@screens/*": ["./screens/*"],
      "@screens": ["./screens"],
      "@components/*": ["./components/*"],
      "@stacks/*": ["./stacks/*"],
      "@hooks/*": ["./hooks/*"],
      "@assets/*": ["./assets/*"],
      "@graphql/*": ["./src/graphql/*"],
      "@lib/*": ["./lib/*"],
      "@auth-boundary/*": ["./auth-boundary/*"],
      "@auth-boundary": ["./auth-boundary"],
      "@modules/*": ["./modules/*"],
      "@event-details-boundary/*": ["./event-details-boundary/*"],
      "@event-details-boundary": ["./event-details-boundary"],
      "@core-root/*": ["./core-root/*"],
      "@core-root": ["./core-root"],
      "@location/*": ["./location/*"],
      "@location": ["./location"],
      "@content-reporting-boundary": ["./content-reporting-boundary"],
      "@date-time": ["./date-time"],
      "@date-time/*": ["./date-time/*"],
      "@test-helpers/*": ["./test-helpers/*"],
      "@shared-models/*": ["./shared-models/*"],
      "@notifications": ["./notifications"],
      "@explore-events-boundary": ["./explore-events-boundary"],
      "@arrival-tracking": ["./arrival-tracking"],
      "@arrival-tracking/*": ["./arrival-tracking/*"],
      "@location-search-boundary": ["./location-search-boundary"],
      "env": ["./.env"]
    }
  },
  "extends": "expo/tsconfig.base",
  "exclude": ["node_modules", "amplify/#current-cloud-backend", "dist"]
}
