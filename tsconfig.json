{
  "compilerOptions": {
    "jsx": "react-native",
    "baseUrl": ".",
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext"],
    "checkJs": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "plugins": [],
    "paths": {
      "@screens/*": ["./screens/*"],
      "@screens": ["./screens"],
      "@components/*": ["./components/*"],
      "@api-client/*": ["./api-client/*"],
      "@api-client": ["./api-client"],
      "@stacks/*": ["./stacks/*"],
      "@hooks/*": ["./hooks/*"],
      "@assets/*": ["./assets/*"],
      "@graphql/*": ["./src/graphql/*"],
      "@lib/*": ["./lib/*"],
      "@auth/*": ["./auth/*"],
      "@auth": ["./auth"],
      "@modules/*": ["./modules/*"],
      "@event-details/*": ["./event-details/*"],
      "@event-details": ["./event-details"],
      "@root-feature/*": ["./root-feature/*"],
      "@root-feature": ["./root-feature"],
      "@location/*": ["./location/*"],
      "@location": ["./location"],
      "@content-parsing": ["./content-parsing"],
      "@content-reporting": ["./content-reporting"],
      "@date-time": ["./date-time"],
      "@test-helpers/*": ["./test-helpers/*"],
      "env": ["./.env"]
    }
  },
  "extends": "expo/tsconfig.base",
  "exclude": ["node_modules", "amplify/#current-cloud-backend", "dist"]
}
