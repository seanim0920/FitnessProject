{
  "cli": {
    "version": ">=5.6.0"
  },
  "build": {
    "development": {
      "env": {
        "BUILD_TYPE": "storybook"
      },
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDevelopmentDebug"
      },
      "ios": {
        "buildConfiguration": "Debug",
        "scheme": "tifapp-storybook"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {},
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    }
  }
}
