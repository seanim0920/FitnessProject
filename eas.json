{
  "cli": {
    "version": ">=5.6.0"
  },
  "build": {
    "development": {
      "env": {
        "EAS_BUILD_TYPE": "development"
      },
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "env": {
        "EAS_BUILD_TYPE": "preview"
      },
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "EAS_BUILD_TYPE": "production"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    }
  }
}
